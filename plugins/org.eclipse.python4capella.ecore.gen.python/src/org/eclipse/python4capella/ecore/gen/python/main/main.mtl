[**
 * Generated Python files for Python4Capella from an Ecore model.
 * @author <a href="mailto:yvan.lussaud@obeo.fr">Yvan Lussaud</a>
 * @version 1.0.0
 * @since 1.0.0
 */]
[module main('http://www.eclipse.org/emf/2002/Ecore')/]

[import org::eclipse::python4capella::ecore::gen::python::files::header/]
[import org::eclipse::python4capella::ecore::gen::python::files::body/]

[**
 * Generates the different Python files for the given EPackage.
 * @param pkg the package to generate
 * @main
 */]
[template public generate(pkg : ecore::EPackage)]
  [pkg.generateHeader()/]
  [pkg.generateBody()/]
[/template]